    "/home/tvp/TVP/inception/badgr/src",
    "/home/tvp/TVP/chromebot_ws/devel/lib/python3/dist-packages",
    "/home/tvp/TVP/bumberbot_ws/devel/lib/python3/dist-packages",
    "/home/tvp/TVP/tvp_ws/devel/lib/python3/dist-packages",
    "/home/tvp/TVP/catkin_ws/devel/lib/python3/dist-packages",
    "/opt/ros/noetic/lib/python3/dist-packages"
  ],
  "python.analysis.extraPaths": [
    "/home/tvp/TVP/inception/badgr/src",
    "/home/tvp/TVP/chromebot_ws/devel/lib/python3/dist-packages",
    "/home/tvp/TVP/bumberbot_ws/devel/lib/python3/dist-packages",
    "/home/tvp/TVP/tvp_ws/devel/lib/python3/dist-packages",
    "/home/tvp/TVP/catkin_ws/devel/lib/python3/dist-packages",
    "/opt/ros/noetic/lib/python3/dist-packages"
  ],
"code-runner.runInTerminal": true,
"code-runner.executorMap": {
  "cpp": "cd $dir && g++ -std=c++17 -Wall -I ../include -g *.cpp -o $dir$fileNameWithoutExt && $dir$fileNameWithoutExt"
},
"[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
},
"code-runner.clearPreviousOutput": true,
"code-runner.customCommand": "",
"code-runner.defaultLanguage": "c++",
"C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
"vsintellicode.features.python.deepLearning": "enabled",



